
@model IEnumerable<IdentityTest2.Models.ApplicationUser>
@{
    ViewBag.Title = "Index";
}

<h2>List of managers</h2>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Username</th>
            <th scope="col">E-mail</th>
            <th scope="col">Edit</th>
            <th scope="col">Delete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var manager in Model)
        {
        <tr>
            <td>@manager.UserName</td>
            <td>@manager.Email</td>
            <td>@Html.ActionLink("Edit", "EditManager", new { id = manager.Id })</td>
            <td>@Html.ActionLink("Delete", "DeleteManager", new { id = manager.Id })</td>
        </tr>
        }
    </tbody>
</table>

@Html.ActionLink("Create manager", "CreateManager", "Admin")

